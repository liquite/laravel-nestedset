{
    "name": "liquite/laravel-nestedset",
    "description": "Nested Set Model for Laravel 8",
    "keywords": ["laravel", "nested sets", "nsm", "database", "hierarchy"],
    "license": "MIT",

    "authors": [
        {
            "name": "Alexander Kalnoy",
            "email": "lazychaser@gmail.com"
        },
        {
            "name": "Patrick Hayder",
            "email": "patrick@hayder.org"
        }
    ],

    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/events": "^7.0|^8.0"
    },

    "autoload": {
        "psr-4": {
            "Liquite\\Nestedset\\": "src/"
        }
    },

    "require-dev": {
        "phpunit/phpunit": "7.*"
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "extra": {
        "branch-alias": {
            "dev-master": "v5.0.x-dev"
        },

        "laravel": {
            "providers": [
                "Liquite\\Nestedset\\NestedSetServiceProvider"
            ]
        }
    }
}
